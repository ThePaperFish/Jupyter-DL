{
  "manifest_version": 2,
 
  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["js/content.js"],
    "matches": ["https://medium.com/*","https://hackernoon.com/*","https://towardsdatascience.com/*"]
  }],
  
  "browser_action": {
        "default_popup": "popup.html",
		"js": ["js/function.js"]
	}
 
}